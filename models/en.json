{
  "invocation": "radio nicasource",
  "version": "4.0",
  "intents": {
    "YesIntent": {
      "phrases": ["yes", "yes please", "sure"]
    },
    "NoIntent": {
      "phrases": ["no", "no thanks"]
    },
    "MyNameIntent": {
      "phrases": ["my name is {name}", "i am {name}", "you can call me {name}", "call me {name}"],
      "entities": {
        "name": {
          "type": {
            "alexa": "AMAZON.FirstName"
          }
        }
      }
    },
    "MyCountryIntent": {
      "phrases": ["i am from {country}", "from {country}"],
      "entities": {
        "country": {
          "type": {
            "alexa": "AMAZON.Country"
          }
        }
      }
    },
    "MyMusicGenreIntent": {
      "phrases": [
        "my favorite music genre {musicGenre}",
        "my favorite genre {musicGenre}",
        "i like {musicGenre} music",
        "i like {musicGenre} genre"
      ],
      "entities": {
        "musicGenre": {
          "type": {
            "alexa": "AMAZON.Genre"
          }
        }
      }
    }
  },
  "alexa": {
    "interactionModel": {
      "languageModel": {
        "intents": [
          {
            "name": "AMAZON.CancelIntent",
            "samples": ["cancel", "never mind", "forget it"]
          },
          {
            "name": "AMAZON.HelpIntent",
            "samples": ["help", "help me", "can you help me"]
          },
          {
            "name": "AMAZON.StopIntent",
            "samples": ["stop", "off", "shut up"]
          },

          {
            "name": "AMAZON.ResumeIntent",
            "samples": []
          },
          {
            "name": "AMAZON.PauseIntent",
            "samples": []
          },
          {
            "name": "AMAZON.StartOverIntent",
            "samples": ["start over", "restart", "start again"]
          },
          {
            "name": "AMAZON.RepeatIntent",
            "samples": ["repeat", "say that again", "repeat that"]
          },
          {
            "name": "AMAZON.FallbackIntent",
            "samples": []
          }
        ]
      }
    }
  }
}
