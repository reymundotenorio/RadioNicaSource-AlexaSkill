{
  "invocation": "radio nicasource",
  "version": "4.0",
  "intents": {
    "YesIntent": {
      "phrases": ["yes", "yes please", "sure"]
    },
    "NoIntent": {
      "phrases": ["no", "no thanks"]
    },
    "MyNameIntent": {
      "phrases": [
        "{name}",
        "my name is {name}",
        "i am {name}",
        "you can call me {name}",
        "call me {name}"
      ],
      "entities": {
        "name": {
          "type": {
            "alexa": "AMAZON.FirstName"
          }
        }
      }
    },
    "MyCountryIntent": {
      "phrases": ["{country}", "i am from {country}", "from {country}"],
      "entities": {
        "country": {
          "type": {
            "alexa": "AMAZON.Country"
          }
        }
      }
    }
  },
  "alexa": {
    "interactionModel": {
      "languageModel": {
        "intents": [
          {
            "name": "AMAZON.CancelIntent",
            "samples": []
          },
          {
            "name": "AMAZON.HelpIntent",
            "samples": []
          },
          {
            "name": "AMAZON.StopIntent",
            "samples": []
          },

          {
            "name": "AMAZON.ResumeIntent",
            "samples": []
          },
          {
            "name": "AMAZON.PauseIntent",
            "samples": []
          }
        ]
      }
    }
  }
}
